    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "latest",
    "nodemailer": "^6.9.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.4.1",
